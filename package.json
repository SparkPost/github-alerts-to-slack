{
  "name": "github-alerts-to-slack",
  "version": "1.0.0",
  "description": "This script is designed to alert on critical and high vulnerabilities found in a list of GitHub repositories. Currently, GitHub does not offer vulnerability reporting across an organization. This script is designed to be a quick way to list any open issues in a single spot.",
  "main": "index.js",
  "dependencies": {
    "@octokit/core": "^3.3.1",
    "@octokit/request": "^5.4.14",
    "@octokit/rest": "^18.3.5",
    "@slack/bolt": "^3.4.0",
    "@slack/socket-mode": "^1.1.0",
    "@slack/web-api": "^6.2.4",
    "bluebird": "^3.7.2",
    "dotenv": "^8.6.0",
    "got": "^11.3.0",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.12",
    "octokit": "^1.0.0-hello-world",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": "16.x"
  }
}
